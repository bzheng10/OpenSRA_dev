					"DefaultValue": "SasakiEtal2022",
					"Options" : {
						"SasakiEtal2022": {
							"NameToDisplay": "Sasaki et al. (2022)",
							"ToDisplay": true,
							"DisplayOrder": [
								"phi_cmt",
