									"ToDisplay": true,
									"DefaultValue": null
								},
								"d_tubing": {
									"NameToDisplay": "d_tubing",
									"VarType": "fixed",
									"DescToDisplay": "[m] outer diameter of tubing",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"casing_flow": {
									"NameToDisplay": "steel_grade",
									"VarType": "fixed",
									"DescToDisplay": "flag for whether well is configured for casing flow: \"True\" or \"False\"",
