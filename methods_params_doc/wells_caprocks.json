							],
							"Return": {
								"Description": "Return: This method returns \"fault shear induced strain\" for production casings and tubings.",
								"ReturnParam": ["pipe_strain"]
							},
							"UpstreamDependency": {
								"Description": "Upstream Dependency: This method requires \"permanent ground deformation [pgdef]\" from the \"Engineering Demand Parameter - Surface Fault Rupture\" tab.",
								"UpstreamParam": ["pgdef"]
							},
							"Params": {
								"phi_cmt": {
									"NameToDisplay": "phi_cmt",
									"VarType": "random",
									"DescToDisplay": "\t[deg] friction angle of cement",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"ucs_cmt": {
									"NameToDisplay": "ucs_cmt",
									"VarType": "random",
									"DescToDisplay": "\t[MPa] uniaxial compressive strength of cement",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"theta": {
									"NameToDisplay": "theta",
									"VarType": "random",
									"DescToDisplay": "\t[deg] well-fault crossing angle",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"w_fc": {
									"NameToDisplay": "w_fc",
									"VarType": "random",
									"DescToDisplay": "\t[m] fault core width",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"w_dz": {
									"NameToDisplay": "w_dz",
									"VarType": "random",
									"DescToDisplay": "\t[m] damage zone width",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"z_crossing": {
									"NameToDisplay": "z_crossing",
									"VarType": "random",
									"DescToDisplay": "\t[m] depth to well-fault crossing",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"e_rock": {
									"NameToDisplay": "e_rock",
									"VarType": "random",
									"DescToDisplay": "\t[GPa] Young's modulus of rock",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"z_top_of_cement": {
									"NameToDisplay": "z_top_of_cement",
									"VarType": "fixed",
									"DescToDisplay": "\t[m] depth to top of cement in well",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"d_production_boring": {
									"NameToDisplay": "d_production_boring",
									"VarType": "fixed",
									"DescToDisplay": "\t[m] diameter of production boring",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"d_production_casing": {
									"NameToDisplay": "d_production_casing",
									"VarType": "fixed",
									"DescToDisplay": "\t[m] outer diameter of production casing",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"d_tubing": {
									"NameToDisplay": "d_tubing",
									"VarType": "fixed",
									"DescToDisplay": "\t[m] outer diameter of tubing",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								},
								"casing_flow": {
									"NameToDisplay": "steel_grade",
									"VarType": "fixed",
									"DescToDisplay": "\tflag for whether well is configured for casing flow: \"True\" or \"False\"",
									"WidgetType": "QLabel",
									"ToDisplay": true,
									"DefaultValue": null
								}
							}
						}
					}
				}
			}
